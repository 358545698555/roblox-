-- Burger Game æ±‰å ¡åŒ… å¿è€…æ³¨å…¥å™¨ 100å€æ”»é€Ÿç»ˆæç‰ˆ 2025.12
-- å¤åˆ¶åˆ°å¿è€…æ³¨å…¥å™¨ç›´æ¥æ‰§è¡Œ

repeat wait() until game:IsLoaded()

local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local root = character:WaitForChild("HumanoidRootPart")

-- å¿è€…æœ€å¼ºç»•è¿‡
hookmetamethod(game, "__namecall", function(self, ...)
    local method = getnamecallmethod()
    if method == "Kick" or tostring(self):lower():find("anticheat") then
        return wait(9e9)
    end
    return hookmetamethod(game, "__namecall")(self, ...)
end)

-- åˆ›å»ºä¸æ»‘UIï¼ˆå¸¦æ”»é€Ÿæ»‘å—ï¼‰
local sg = Instance.new("ScreenGui", player.PlayerGui)
sg.ResetOnSpawn = false

local frame = Instance.new("Frame", sg)
frame.Size = UDim2.new(0, 350, 0, 220)
frame.Position = UDim2.new(0, 100, 0, 100)
frame.BackgroundColor3 = Color3.fromRGB(20,20,25)
frame.Active = true; frame.Draggable = true

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1,0,0,40); title.Text = "ğŸ” æ±‰å ¡åŒ… 100å€æ”»é€Ÿç¥å™¨"
title.BackgroundColor3 = Color3.fromRGB(255,100,0); title.TextColor3 = Color3.new(1,1,1)
title.Font = Enum.Font.GothamBold

-- æ”»é€Ÿæ»‘å—
local speedLabel = Instance.new("TextLabel", frame)
speedLabel.Size = UDim2.new(1,-20,0,30); speedLabel.Position = UDim2.new(0,10,0,50)
speedLabel.Text = "åšæ±‰å ¡é€Ÿåº¦: 50x"; speedLabel.BackgroundTransparency = 1
speedLabel.TextColor3 = Color3.new(0,1,1)

local slider = Instance.new("TextButton", frame)
slider.Size = UDim2.new(0,300,0,30); slider.Position = UDim2.new(0,25,0,80)
slider.BackgroundColor3 = Color3.fromRGB(60,60,60); slider.Text = ""

local fill = Instance.new("Frame", slider)
fill.Size = UDim2.new(0.5,0,1,0); fill.BackgroundColor3 = Color3.fromRGB(0,255,100)

getgenv().FarmSpeed = 50  -- é»˜è®¤50å€é€Ÿï¼ˆå¯è°ƒ1~100ï¼‰

slider.MouseButton1Down:Connect(function()
    local mouse = player:GetMouse()
    local connection
    connection = mouse.Move:Connect(function()
        local relativeX = math.clamp((mouse.X - slider.AbsolutePosition.X) / slider.AbsoluteSize.X, 0, 1)
        fill.Size = UDim2.new(relativeX, 0, 1, 0)
        getgenv().FarmSpeed = math.floor(relativeX * 99) + 1
        speedLabel.Text = "åšæ±‰å ¡é€Ÿåº¦: "..getgenv().FarmSpeed.."x"
    end)
    mouse.Button1Up:Connect(function()
        connection:Disconnect()
    end)
end)

-- ä¸»æ”»é€Ÿå¾ªç¯ï¼ˆæ ¸å¿ƒï¼ï¼‰
spawn(function()
    while wait(0.01) do  -- è¶…é«˜é¢‘å¾ªç¯
        if not getgenv().FarmSpeed then getgenv().FarmSpeed = 50 end
        for i = 1, getgenv().FarmSpeed do
            pcall(function()
                -- è‡ªåŠ¨å‡çº§åº—é“º
                for _, v in pairs(Workspace:GetDescendants()) do
                    if v:IsA("ProximityPrompt") and (string.find(v.ObjectText:lower(),"upgrade") or string.find(v.ObjectText:lower(),"buy") or string.find(v.ObjectText:lower(),"hire")) then
                        fireproximityprompt(v, true)
                    end
                end

                -- æ— é™é£Ÿæ + åšæ±‰å ¡
                for _, v in pairs(Workspace:GetDescendants()) do
                    if v:IsA("ProximityPrompt") and string.find(v.ObjectText:lower(),"bread") or string.find(v.ObjectText:lower(),"patty") then
                        fireproximityprompt(v, true)
                    end
                    if v.Name:find("Grill") or v.Name:find("Counter") then
                        firetouchinterest(root, v, 0)
                        firetouchinterest(root, v, 1)
                    end
                end

                -- é€å•
                for _, cust in pairs(Workspace.Customers:GetChildren()) do
                    if cust:FindFirstChild("HumanoidRootPart") then
                        root.CFrame = cust.HumanoidRootPart.CFrame * CFrame.new(0,0,-3)
                        for _, p in pairs(cust:GetDescendants()) do
                            if p:IsA("ProximityPrompt") and p.ObjectText == "Serve" then
                                fireproximityprompt(p, true)
                            end
                        end
                    end
                end
            end)
        end
    end
end)

-- é€šçŸ¥
game.StarterGui:SetCore("SendNotification", {
    Title = "å¿è€…æ±‰å ¡åŒ…ç¥å™¨";
    Text = "100å€æ”»é€Ÿå·²å¼€å¯ï¼æ‹–åŠ¨æ»‘å—è°ƒèŠ‚é€Ÿåº¦ï¼ˆæœ€é«˜100xï¼‰\nè‡ªåŠ¨å‡çº§ + è‡ªåŠ¨åšæ±‰å ¡ + é€å•å…¨å¼€ï¼";
    Duration = 12;
})

print("æ±‰å ¡åŒ…100å€æ”»é€Ÿè„šæœ¬æ³¨å…¥æˆåŠŸï¼æ‹–æ»‘å—è°ƒèŠ‚é€Ÿåº¦ï¼Œèººç€å½“é¦–å¯Œï¼")
